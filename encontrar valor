#include <iostream>
#include <string.h>

using namespace std;

int main() {
	int notas[4] = { 20, 18, 10, 11 };
	int notas_buscar;
	bool encontrado = false;
	cout << "Ingrese la nota a buscar: "; cin >> notas_buscar;
	for (int i = 0; i < 4; i++) {
		if (notas[i] == notas_buscar) {
			cout << "Valor encontrado";
			encontrado = true;
		}
	}
	if (!encontrado)
	{
		cout << "No se encontro el valor solicitado";
	}
}
